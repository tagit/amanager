{{#bs-navbar fluid=true type="inverse" position="fixed-top" class=class}}
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      {{#bs-navbar-toggle}}
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
      {{/bs-navbar-toggle}}
      {{#link-to 'index' class="navbar-brand"}}AManager{{/link-to}}
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    {{#bs-navbar-content}}
      {{yield}}
      {{#bs-navbar-nav class="navbar-right"}}
          <li><a><i class="ion-ios-gear"></i></a></li>
        {{#bs-dropdown tagName="li"}}
          {{#bs-dropdown-toggle}}My Company <span class="caret"></span>{{/bs-dropdown-toggle}}
          {{#bs-dropdown-menu}}
              <li><a>Logout</a></li>
          {{/bs-dropdown-menu}}
        {{/bs-dropdown}}
      {{/bs-navbar-nav}}
    {{/bs-navbar-content}}
{{/bs-navbar}}
